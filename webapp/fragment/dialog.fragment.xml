<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
    <Dialog
        id="airlineDialog"
        title="Airline Details">

        <VBox id="_IDGenVBox1" class="sapUiSmallMargin">
            <Label id="_IDGenLabel" text="Carrid" />
            <Text id="_IDGenText5" text="{mainService>Carrid}" />

            <Label id="_IDGenLabel1" text="Carrname" />
            <Text id="_IDGenText6" text="{mainService>Carrname}" />

            <Label id="_IDGenLabel2" text="URL" />
            <Link id="_IDGenLink1" text="{mainService>Url}" href="{mainService>Url}" target="_blank" />
        </VBox>

        <endButton>
            <Button id="_IDGenButton" text="Close" press=".onCloseDialog" />
        </endButton>
    </Dialog>
</core:FragmentDefinition>
